<div id="youtube-repeat" class="page">
  {{input id="youtube-search" value=query placeholder="search on youtube..."}}
  <ul id="suggestion-list">
    {{#each song in model itemController="song"}}
      {{#with song as item}}
        <li {{action "select"}}>
          <div>{{model.title}}</div>
        </li>
      {{/with}}
    {{/each}}
  </ul>
</div>
